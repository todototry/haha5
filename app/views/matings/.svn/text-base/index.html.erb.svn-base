<h1>Listing matings</h1>

<table>
  <tr>
    <th>Com</th>
    <th>Momid</th>
    <th>Dadid</th>
    <th>Time</th>
    <th>State</th>
    <th>Sum</th>
    <th>Daughternum</th>
    <th>Note</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @matings.each do |mating| %>
  <tr>
    <td><%= mating.com_id %></td>
    <td><%= mating.momid %></td>
    <td><%= mating.dadid %></td>
    <td><%= mating.time %></td>
    <td><%= mating.state %></td>
    <td><%= mating.sum %></td>
    <td><%= mating.daughternum %></td>
    <td><%= mating.note %></td>
    <td><%= link_to 'Show', mating %></td>
    <td><%= link_to 'Edit', edit_mating_path(mating) %></td>
    <td><%= link_to 'Destroy', mating, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Mating', new_mating_path %>
